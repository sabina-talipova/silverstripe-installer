{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-plugin": "2.x-dev",
        "silverstripe/vendor-plugin": "2.x-dev",
        "silverstripe/recipe-cms": "5.x-dev",
        "silverstripe-themes/simple": "3.x-dev",
        "silverstripe/login-forms": "5.x-dev",
        "silverstripe/cms": "dev-pulls/5/versioned-ui as 5.x-dev",
        "silverstripe/framework": "dev-pulls/5/versioned-ui as 5.x-dev",
        "silverstripe/versioned": "dev-pulls/2/versioned-ui as 2.x-dev",
        "silverstripe/frameworktest": "dev-pulls/1/versioned-ui-test-objects as 1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "extra": {
        "project-files": [
            "app/_config/*",
            ".env.example",
            ".graphql-generated/*"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ]
    },
    "repositories": {
        "x1": {
            "type": "vcs",
            "url": "https://github.com/creative-commoners/silverstripe-cms"
        },
        "x2": {
            "type": "vcs",
            "url": "https://github.com/creative-commoners/silverstripe-framework"
        },
        "x3": {
            "type": "vcs",
            "url": "https://github.com/creative-commoners/silverstripe-versioned"
        },
        "x4": {
            "type": "vcs",
            "url": "https://github.com/creative-commoners/silverstripe-frameworktest"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
